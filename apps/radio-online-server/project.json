{
  "name": "radio-online-server",
  "root": "apps/radio-online-server",
  "sourceRoot": "apps/radio-online-server",
  "projectType": "application",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/radio-online-server",
        "commands": [
          "docker compose down",
          "docker compose build --no-cache",
          "docker compose up -d"
        ],
        "parallel": false
      }
    }
  },
  "tags": []
} 